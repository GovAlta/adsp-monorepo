{
    "kind": "collectionType",
    "collectionName": "service_releases",
    "info": {
        "singularName": "service-release",
        "pluralName": "service-releases",
        "displayName": "Service release",
        "description": ""
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "tenantId": {
            "type": "string"
        },
        "releasedOn": {
            "type": "date",
            "required": true
        },
        "notes": {
            "type": "blocks",
            "required": true
        },
        "version": {
            "type": "string",
            "required": true
        },
        "service": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::service.service",
            "inversedBy": "releases"
        }
    }
}
