{
    "kind": "collectionType",
    "collectionName": "services",
    "info": {
        "singularName": "service",
        "pluralName": "services",
        "displayName": "Service",
        "description": ""
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "tenantId": {
            "type": "string"
        },
        "name": {
            "type": "string",
            "required": true
        },
        "description": {
            "type": "text"
        },
        "maturity": {
            "type": "enumeration",
            "enum": [
                "Alpha",
                "Beta",
                "Live"
            ]
        },
        "releases": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::service-release.service-release",
            "mappedBy": "service"
        }
    }
}
