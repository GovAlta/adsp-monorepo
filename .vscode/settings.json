{
  "cucumberautocomplete.steps": [
    "apps/tenant-management-webapp-e2e/src/integration/**/*.ts",
    "apps/status-app-e2e/src/integration/**/*.ts",
    "apps/subscriber-app-e2e/src/integration/**/*.ts",
    "apps/form-app-e2e/src/integration/**/*.ts"
  ],
  "cucumberautocomplete.syncfeatures": "apps/tenant-management-webapp-e2e/src/integration/**/*.feature",
  "cucumberautocomplete.strictGherkinCompletion": false,
  "cucumberautocomplete.smartSnippets": true,
  "cucumberautocomplete.stepsInvariants": true,
  "editor.formatOnSave": true,
  "editor.trimAutoWhitespace": true,
  "editor.tabSize": 2,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**/node_modules": true
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp",
    "editor.formatOnType": true,
  },
  "cSpell.words": [
    "abgov",
    "Adsp",
    "Albertans",
    "botbuilder",
    "ccds",
    "Checkmark",
    "clamdscan",
    "clamscan",
    "createkc",
    "dashify",
    "datetime",
    "envalid",
    "eventnamespace",
    "filespace",
    "hasha",
    "healthcheck",
    "instanceof",
    "jsonforms",
    "jwks",
    "keycloak",
    "mailtrap",
    "metascan",
    "minlength",
    "mongod",
    "notificationtype",
    "Objs",
    "oidc",
    "openid",
    "originalname",
    "persistor",
    "reduxjs",
    "signin",
    "testid",
    "traceparent",
    "uninstantiated",
    "upsert",
    "uuidv"
  ],
  "omnisharp.enableRoslynAnalyzers": true,
  "dotnet-test-explorer.testProjectPath": "libs/adsp-service-net-sdk/",
  "java.configuration.updateBuildConfiguration": "automatic",
  "java.compile.nullAnalysis.mode": "automatic"
}