{
  "branches": [
    "+([0-9])?(.{+([0-9]),x}).x",
    "main",
    {
      "name": "beta",
      "prerelease": true
    },
    {
      "name": "alpha",
      "prerelease": true
    }
  ],
  "tagFormat": "adsp-service-spring-sdk-v${version}",
  "plugins": [
    [
      "@abgov/nx-release",
      {
        "project": "adsp-service-spring-sdk"
      }
    ],
    "@conveyal/maven-semantic-release",
    [
      "@semantic-release/github",
      {
        "releasedLabels": false,
        "successComment": false
      }
    ]
  ]
}