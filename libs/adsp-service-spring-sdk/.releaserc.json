{
  "branches": [
    "+([0-9])?(.{+([0-9]),x}).x",
    "main",
    {
      "name": "beta",
      "prerelease": true
    },
    {
      "name": "alpha",
      "prerelease": true
    }
  ],
  "tagFormat": "adsp-service-spring-sdk-v${version}",
  "plugins": [
    [
      "@abgov/nx-release",
      {
        "project": "adsp-service-spring-sdk"
      }
    ],
    [
      "@semantic-release/github",
      {
        "releasedLabels": false,
        "successComment": false,
        "assets": "dist/libs/adsp-service-spring-sdk/"
      }
    ]
  ]
}