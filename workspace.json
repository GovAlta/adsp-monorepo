{
  "version": 1,
  "projects": {
    "file-service": {
      "root": "apps/file-service",
      "sourceRoot": "apps/file-service/src",
      "projectType": "application",
      "prefix": "file-service",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "webpackConfig": "apps/file-service/webpack.config.js",
            "outputPath": "dist/apps/file-service",
            "main": "apps/file-service/src/main.ts",
            "tsConfig": "apps/file-service/tsconfig.app.json",
            "assets": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": []
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "file-service:build",
            "port": 7770
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/file-service/tsconfig.app.json",
              "apps/file-service/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/file-service/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/file-service/jest.config.js",
            "tsConfig": "apps/file-service/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
