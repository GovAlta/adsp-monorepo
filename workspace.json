    "tenant-management-webapp": {
      "root": "apps/tenant-management-webapp",
      "sourceRoot": "apps/tenant-management-webapp/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/tenant-management-webapp",
            "index": "apps/tenant-management-webapp/src/index.html",
            "main": "apps/tenant-management-webapp/src/main.tsx",
            "polyfills": "apps/tenant-management-webapp/src/polyfills.ts",
            "tsConfig": "apps/tenant-management-webapp/tsconfig.app.json",
            "assets": [
              "apps/tenant-management-webapp/src/favicon.ico",
              "apps/tenant-management-webapp/src/assets",
              {
                "glob": "nginx.conf",
                "input": "./",
                "output": "./"
              }
            ],
            "styles": ["apps/tenant-management-webapp/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/tenant-management-webapp/src/environments/environment.ts",
                  "with": "apps/tenant-management-webapp/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "tenant-management-webapp:build",
            "proxyConfig": "apps/tenant-management-webapp/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "tenant-management-webapp:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/tenant-management-webapp/tsconfig.app.json",
              "apps/tenant-management-webapp/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/tenant-management-webapp/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/tenant-management-webapp/jest.config.js",
            "tsConfig": "apps/tenant-management-webapp/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "tenant-management-webapp-e2e": {
